{
    "name": "orwallet/foxtrot-sdk",
    "require-dev": {
        "laravel/framework": "^10.0",
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.16"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Orwallet\\FoxtrotSdk\\": "src/",
            "Orwallet\\FoxtrotSdk\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jeus Bausa",
            "email": "jeus@orientalwallet.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^7.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orwallet\\FoxtrotSdk\\ServiceProvider\\FoxtrotServiceProvider"
            ],
            "aliases": {
                "Foxtrot": "Orwallet\\FoxtrotSdk\\Facade\\Foxtrot"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
